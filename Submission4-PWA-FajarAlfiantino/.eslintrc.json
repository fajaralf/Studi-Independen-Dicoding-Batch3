{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [0 ,"error", "windows"],
        "no-console": 0,
        "operator-linebreak" : "off",
        "no-return-await" : "off",
        "no-unused-expressions" : "off",
        "object-curly-newline" : "off"
    }
}
